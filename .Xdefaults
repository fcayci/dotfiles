*.color240: nord0

Xft*dpi:                    96
Xft*antialias:              true
Xft*hinting:                full
!Xft.hinting:                false
!Xft.hintstyle:              hintslight

! URXVT Settings
URxvt.perl-ext-common: tabbed

! dont scroll with the output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
! disable scrollback buffer for the secondary screen
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

URxvt*saveLines: 12000

! Disable printing the terminal contents when pressing PrintScreen.
URxvt.print-pipe: "cat > /dev/null"

!URxvt.clipboard.autocopy: true
!URxvt.keysym.Shift-Control-c: perl:clipboard:copy
URxvt.keysym.Shift-Control-C: eval:selection-to-clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 4
URxvt.tabbed.tab-bg: 0

!URxvt.depth:                32
!URxvt.loginShell:           true
!URxvt.internalBorder:       3
!URxvt.lineSpace:            0

!URxvt*font:            xft:Dejavu Sans Mono for Powerline:size=14
!URxvt*font:            xft:Inconsolata for Powerline:size=12
!URxvt*font:            xft:Noto Mono for Powerline:size=10
URxvt*font:            xft:Source Code Pro for Powerline:size=10
!URxvt*font:            xft:Terminus:pixelsize=12,xft:Roboto Mono for Powerline:pixelsize=10
!URxvt*font:            xft:Terminus:pixelsize=14
!URxvt*boldFont:        xft:Terminus:bold:pixelsize=12
!URxvt.font: xft:Consolas:size=13, xft:Inconsolata for Powerline
!URxvt*font: xft:Terminus:pixelsize=12,xft:Inconsolata\ for\ Powerline:pixelsize=12

!URxvt.letterSpace: 0
!URxvt.lineSpace: 0
!URxvt.internalBorder: 24
!URxvt.cursorBlink: true
!URxvt.cursorUnderline: false
URxvt.scrollBar: false
URxvt.scrollBar_right: false
!URxvt.urgentOnBell: true
URxvt.iso14755: false
URxvt.iso14755_52: false

!URxvt.perl-ext-common:  clipboard
!URxvt.clipboard.autocopy: true
!URxvt.keysym.Shift-Conrol-c: perl:clipboard:copy
!URxvt.keysym.Shift-Control-v: perl:clipboard:paste

