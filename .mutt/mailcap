# muttrc
#
# MS Word documents
application/msword; xdg-open %s

# Images
image/jpg; feh %s
image/jpeg; feh %s
image/png; feh %s
image/gif; feh %s

# PDFs
application/pdf; xdg-open %s

# HTML
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -collapse_br_tags -dump %s; nametemplate=%s.html; copiousoutput

# Unidentified files
#application/octet-stream;
